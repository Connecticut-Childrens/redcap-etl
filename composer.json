{
    "name" : "iu-redcap/redcap-etl",
    "license" : "Apache-2.0",
    "description" : "REDCap-ETL",
    "keywords" : [
        "REDCap", "ETL"
    ],
    "require" : {
        "php" : ">=5.6.0",
        "ext-curl" : "*",
        "ext-dom" : "*",
        "ext-openssl" : "*",
        "phpcap/phpcap" : "^0.3.2"
    },
    "require-dev" : {
        "apigen/apigen" : "v4.1.2",
        "phpunit/phpunit" : "6.5.8",
        "squizlabs/php_codesniffer" : "3.2.3",
        "kzykhys/php-csv-parser" : "1.4.1"
    },
    "autoload" : {
        "psr-4" : {
            "IU\\REDCapETL\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "IU\\REDCapETL\\" : [
                "tests/unit",
                "tests/integration",
                "tests/system"
            ]
        }
    }
}

